{
  "kind": "collectionType",
  "collectionName": "event_categories",
  "info": {
    "singularName": "event-category",
    "pluralName": "event-categories",
    "displayName": "Event Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "categoryName": {
      "type": "string",
      "required": true
    },
    "categoryId": {
      "type": "string",
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "categories"
    }
  }
}
