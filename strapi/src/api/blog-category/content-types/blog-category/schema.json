{
  "kind": "collectionType",
  "collectionName": "blog_categories",
  "info": {
    "singularName": "blog-category",
    "pluralName": "blog-categories",
    "displayName": "Blog Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "enumeration",
      "enum": [
        "house",
        "job",
        "life",
        "school",
        "other"
      ],
      "default": "other",
      "required": true
    },
    "blog": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog.blog"
    }
  }
}
