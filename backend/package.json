{
  "name": "express-mvc-starter1",
  "version": "1.0.0",
  "main": "index.js",
  "files": [
    "src/",
    ".env.example",
    ".gitignore"
  ],
  "scripts": {
    "setup": "npm i",
    "server": "NODE_ENV=development tsnd --inspect -- src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "dotenv -e .env.development -- prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.development -- prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "An Express MVC setup tool",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@prisma/client": "^6.2.1",
    "@types/node-cron": "^3.0.11",
    "apicache": "^1.6.3",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.6",
    "prisma": "^6.2.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
